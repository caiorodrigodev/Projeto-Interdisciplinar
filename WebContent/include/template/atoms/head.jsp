    <style>
    
.rainbow {
  
   /* Font options */
  font-family: 'Calibri', cursive;
  
   /* Chrome, Safari, Opera */
  -webkit-animation: rainbow 5s infinite; 
  
  /* Internet Explorer */
  -ms-animation: rainbow 5s infinite;
  
  /* Standar Syntax */
  animation: rainbow 5s infinite; 
}

/* Chrome, Safari, Opera */
@-webkit-keyframes rainbow{
  0%{color: #00ccff;}	
  10%{color: #00b4ff;}	
	20%{color: #009cff;}
  30%{color: #008aff;}
	40%{color: #0078ff;}
  50%{color: #0072ff;}
	60%{color: #0054ff;}
  70%{color: #0042ff;}
  80%{color: #002aff;}
  90%{color: #001eff;}
	100%{color: #00ccff;}
}

/* Internet Explorer */
@-ms-keyframes rainbow{
  0%{color: #00ccff;}	
  10%{color: #00b4ff;}	
	20%{color: #009cff;}
  30%{color: #008aff;}
	40%{color: #0078ff;}
  50%{color: #0072ff;}
	60%{color: #0054ff;}
  70%{color: #0042ff;}
  80%{color: #002aff;}
  90%{color: #001eff;}
	100%{color: #00ccff;}
}

/* Standar Syntax */
@keyframes rainbow{
  0%{color: #00ccff;}	
  10%{color: #00b4ff;}	
	20%{color: #009cff;}
  30%{color: #008aff;}
	40%{color: #0078ff;}
  50%{color: #0072ff;}
	60%{color: #0054ff;}
  70%{color: #0042ff;}
  80%{color: #002aff;}
  90%{color: #001eff;}
	100%{color: #00ccff;}
}
    
